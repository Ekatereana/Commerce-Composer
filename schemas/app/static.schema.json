{
    "$schema": "https://json-schema.org/draft/",
    "$id": "/app/static.schema.json",
    "title": "Configuration for github version control",
    "description": "Set of configurations to define templates for landing pages",
    "$vocabulary": {
        "https://json-schema.org/draft/2020-12/vocab/core": true,
        "https://json-schema.org/draft/2020-12/vocab/validation": true
    },
    "required": [
        "app_type",
        "template"
    ],
    "type": "object",
    "properties": {
        "app_type": {
            "type": "string",
            "enum": ["static"]
        },
        "template": {
            "type": "string",
            "description": "names of available landing page templates",
            "enum": ["knight"]
        }
    }
}